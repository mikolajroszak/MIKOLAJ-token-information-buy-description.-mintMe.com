  "use strict"

  function loadBoxes() {
    if (window.SHOGUN_BOX) {
      window.SHOGUN_BOX.forEach(function(box) {
        if (!box.url) return
        var BOX_SELECTOR = "#" + box.id
        var $box = $(BOX_SELECTOR)

        /* don't bubble box's children anchor clicks */
        $(BOX_SELECTOR + " a").click(function(e) {
          e.stopPropagation()
        })

        /* handle box click event */
